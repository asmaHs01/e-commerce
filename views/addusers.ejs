   
      <%- include('partsadmin/top')%>
            <div class="row">
                <%- include('partsadmin/navbar')%>
            </div>      
      
      <div class="row tm-mt-big">
            <div class="col-xl-8 col-lg-10 col-md-12 col-sm-12">
                <div class="bg-white tm-block">
                    <div class="row">
                        <div class="col-12">
                            <h2 class="tm-block-title d-inline-block">Add user</h2>
                        </div>
                    </div>
                    <div class="row mt-4 tm-edit-product-row">
                        <div class="col-xl-7 col-lg-7 col-md-12">
                            <form method="POST" action="/admin/addusers" enctype="" class="tm-edit-product-form">
                        <div class="input-group mb-3">
                            <label for="firstname" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">User First Name</label>
                            <input type="text" id="firstname" name="firstname" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7" required>
                        </div>
                        <div class="input-group mb-3">
                            <label for="lastname" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">User Last Name</label>
                            <input type="text" id="lastname" name="lastname" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7" required>
                        </div>
                        <div class="input-group mb-3">
                            <label for="email" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">User Email</label>
                            <input type="email" id="email" name="email" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7" required>
                        </div>
                        <div class="input-group mb-3">
                            <label for="age" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">User Age</label>
                            <input type="number" id="age" name="age" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7" required>
                        </div>
                        <div class="input-group mb-3">
                            <label for="password" class="col-xl-4 col-lg-4 col-md-4 col-sm-5 col-form-label">User Password</label>
                            <input type="password" id="password" name="password" class="form-control validate col-xl-9 col-lg-8 col-md-8 col-sm-7" required>
                        </div>
                        <div class="input-group mb-3">
                            <div class="ml-auto col-xl-8 col-lg-8 col-md-8 col-sm-7 pl-0">
                                <button type="submit" class="btn btn-primary">Add</button>
                            </div>
                        </div>
                    </form>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
 
      
      
      
      <%- include ('partsadmin/footer') %>
        </div>
    </div>
    

     <%- include ('partsadmin/bottom') %>