<%- include('parts/top.ejs')%>

<div class="section events" id="events">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <div class="section-heading">
            <h6>Schedule</h6>
            <h2>Details du service</h2>
          </div>
        </div>
        <div class="col-lg-12 col-md-6">
          <div class="item">
            <div class="row">
              <div class="col-lg-3">
                <div class="image">
                  <img src="/uploads/<%= service.image %>" alt="">
                </div>
              </div>
              <div class="col-lg-9">
                <ul>
                  <li>
                    
                    <h4><%= service.nom_service %></h4>
                  </li>
                  <li>
                    <span>Description :</span>
                    <h6><%= service.description %></h6>
                  </li>
                   <li>
                    <span>Memoire:</span>
                    <h6><%= service.prix %>Go</h6>
                  </li>
                  <li>
                    <span>Capacite de stockage:</span>
                    <h6> <%= service.capacite_stockage %> </h6>
                  </li>
                </ul>
                <a href="/"><i class="fa fa-angle-right"></i></a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    </div>

<%- include('parts/bottom.ejs')%>

