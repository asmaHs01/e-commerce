     <div class="row tm-mt-big">
            <div class="col-xl-8 col-lg-10 col-md-12 col-sm-12">
                <div class="bg-white tm-block">
                    <div class="row">
                        <div class="col-12">
                            <h2 class="tm-block-title d-inline-block">Update contact page</h2>
                        </div>
                    </div>
                    <div class="row mt-4 tm-edit-product-row">
                        <div class="col-xl-7 col-lg-7 col-md-12">
                            <form action="/admin/updatecontact/<%= contact._id %>" method="post">
                                <input type="hidden" name="contactId" value="<%= contact._id %>">
                                <div class="form-group">
                                    <label for="title">Title</label>
                                    <input type="text" name="titre_contact" id="titre_contact" class="form-control" value="<%= contact.titre_contact %>">
                                </div>
                                <div class="form-group">
                                    <label for="text_contact">Text</label>
                                    <textarea name="text_contact" id="text_contact" class="form-control"><%= contact.text_contact %></textarea>
                                </div>
                                <button type="submit" class="btn btn-primary">Update Contact</button>
                            </form>
                        </div>
                    </div>      
                </div>
            </div>
        </div>





       


        
              






